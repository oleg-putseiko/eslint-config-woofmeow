{
  "name": "eslint-config-woofmeow",
  "version": "2.0.0",
  "description": "ESLint config for various projects",
  "license": "MIT",
  "author": "Oleg Putseiko <oleg.putseiko@gmail.com> (https://github.com/oleg-putseiko)",
  "keywords": [
    "eslint",
    "eslintrc",
    "config",
    "javascript",
    "typescript",
    "next",
    "react",
    "import",
    "fsd"
  ],
  "homepage": "https://github.com/oleg-putseiko/eslint-config-woofmeow",
  "repository": {
    "type": "git",
    "url": "https://github.com/oleg-putseiko/eslint-config-woofmeow"
  },
  "bugs": {
    "url": "https://github.com/oleg-putseiko/eslint-config-woofmeow/issues"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./general": "./presets/general.js",
    "./import": "./presets/import-base.js",
    "./import-atomic": "./presets/import-atomic.js",
    "./import-fsd": "./presets/import-fsd.js",
    "./next": "./presets/next.js",
    "./react": "./presets/react.js",
    "./typescript": "./presets/typescript.js"
  },
  "scripts": {
    "lint": "eslint ./index.{js,ts,json} ./presets/**/*.{js,ts,json} && yarn format",
    "lint:fix": "eslint ./index.{js,ts,json} ./presets/**/*.{js,ts,json} --fix && yarn format:fix",
    "lint:strict": "eslint ./index.{js,ts,json} ./presets/**/*.{js,ts,json} --max-warnings=0 && yarn format",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "prepare": "husky install | chmod +x ./.husky/*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint-config-next": "^15.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "peerDependencies": {
    "eslint": ">=3.0.0",
    "typescript": ">=3.3.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "eslint": "^9.16.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "yarn": ">=3.6.3"
  },
  "volta": {
    "node": "18.18.0",
    "yarn": "3.6.4"
  },
  "packageManager": "yarn@3.6.4"
}
